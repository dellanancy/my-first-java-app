public class MathsClass {

    public static void main(String args[]) {

    //call the method by the method name
        addition();
        //cal the method by the method name
        substraction();


    }


    //Addition method.
    public static void addition() {

        //Declare a variable
        int a;
        int b;
        //initialise variable
        a =10;
        b=5;
        int answer=a+b;
        System.out.println(a+b);
    }
    //Substraction method.
    public static void substraction(){

        //Declare a variable
        int a;
        int b;
        //initialise variable
        a=10;
        b=5;
        int answer=a-b;
        System.out.println( a-b);
    }


}
